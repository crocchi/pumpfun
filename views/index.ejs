<html>
	<head>
		<title>ScrockBot</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="../public/assets/css/main.css" />
	</head>
	<body class="is-preload left-sidebar">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header-wrapper">
					<header id="header" class="container">

						<!-- Logo -->
							<div id="logo">
								<h1>ScrockBot</a></h1>
								<span>SolanaSniper Bot</span>
							</div>

						<!-- Nav -->
							<nav id="nav">
								<ul>

                                     <label>Min Liquidity (SOL)</label>
                                     <input type="number" step="0.10" id="liquidityMin">
                                    <li><a href="right-sidebar.html">Right Sidebar</a></li>
									<li><a href="no-sidebar.html">No Sidebar</a></li>
								</ul>
							</nav>

					</header>
				</div>



			

<!-- Features -->
				<div id="features-wrapper">
					<div class="container">
						<div class="row">


							<div class="col-4 col-12-medium">

								<!-- Box -->
									<section class="box feature">
										<!--<a href="#" class="image featured"><img src="../public/images/pic01.jpg" alt="" /></a>-->
                                        <div class="inner">
                                            <header>
                                                <h2>Token Monitor</h2>
                                                <p>Monitora i token in base ai filtri impostati</p>
                                            </header>
                                            <ul class="checklist">
                         <% tokensMonitor.forEach(function(monitor, index) { %>
                          <li>
                            <strong><%= monitor.token.name %> (<%= monitor.token.symbol %>)</strong><br>
                              Prezzo: $<%= monitor.price %>  | Market Cap Sol: $<%= monitor.token.marketCapSol.toFixed(4) %> SOL<br>
                              Net Volume: <%= monitor.solAmount.toFixed(4) %> SOL | TradesNum: <%= monitor.solTrxNumMonitor %><br>
                              MonitorStatus: <%= monitor.tradeMonitor %>
                            </li>
                         <% }); %>
                        </ul>
                        </div>
			            </section>

			            </div>



							<div class="col-4 col-12-medium">

								<!-- Box -->
									<section class="box feature">
										<!--<a href="#" class="image featured"><img src="../public/images/pic02.jpg" alt="" /></a>-->
										<div class="inner">
											 <header>
                                                <h2>Token Logger</h2>
                                                <p>Log dei token intercettati</p>
                                            </header>
                                            <ul class="checklist">
                          <% tokenLogger.forEach(function(token, index) { %>
                            <li>
                              <strong><%= token.name %> (<%= token.symbol %>)</strong><br>
                              Prezzo: $<%= token.LivePrice %> | Liquidity: <%= token.solInPool %> SOL | Market Cap Sol: $<%= token.marketCapSol %><br>
                              VolumeNet: $<%= token.volumeNet %> | TradesNum: <%= token.solTrxNumMonitor %> Volume: <%= token.volume %> <br>   
                              Buy Price: <%= token.buyPrice %> | Start Price: <%= token.startPrice %> |
                            </li>
                         <% }); %>
                        </ul>
                        </div>
			            </section>

			            </div>

                       
						</div>
					</div>
				</div>


			<!-- Footer -->
				<div id="footer-wrapper">
					<footer id="footer" class="container">
						<div class="row">
							<div class="col-3 col-6-medium col-12-small">

								<!-- Links -->
									<section class="widget links">
										<h3>Solana Network info</h3>
                                        <ul class="style2"></ul>

                                      
                                      
                                        <li>Prezzo SOL: $<%= botOptions.solanaInfo.price %> </li>
                                       <li>Market Cap: $<%= botOptions.solanaInfo.market_cap %></li>
                                       <li>1h: <%= botOptions.solanaInfo.percent_change_1h %>%</li>
                                        <li>24h: $<%= botOptions.solanaInfo.percent_change_24h %>%</li>
								
											
										</ul>
									</section>

							</div>
							<div class="col-3 col-6-medium col-12-small">

								<!-- Links -->
									<section class="widget links">
										<h3>Bot Info</h3>
										<ul class="style2">
											<li><a href="#">Etiam feugiat condimentum</a></li>
											<li><a href="#">Aliquam imperdiet suscipit odio</a></li>
										</ul>
									</section>

							</div>
						
			
						</div>
						
					</footer>
				</div>

			</div>

		<!-- Scripts -->

			<script src="../public/assets/js/jquery.min.js"></script>
			<script src="../public/assets/js/jquery.dropotron.min.js"></script>
			<script src="../public/assets/js/browser.min.js"></script>
			<script src="../public/assets/js/breakpoints.min.js"></script>
			<!-- <script src="../public/assets/js/util.js"></script>-->
			<script src="../public/assets/js/main.js"></script>

	</body>
</html>